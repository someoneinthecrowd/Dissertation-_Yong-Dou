doi = {10.1103/PhysRevE.94.062203},
eprint = {1610.00296},
file = {:Users/kjmbishop/Library/Application Support/Mendeley Desktop/Downloaded/Ottino-Loffler, Strogatz - 2016 - Comparing the Locking Threshold for Rings and Chains of Oscillators.pdf:pdf},
journal = {Phys. Rev. E},
